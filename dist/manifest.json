{
  "manifest_version": 2,
  "name": "Bmux",
  "version": "0.0.1",

  "description": "Tmux for browser",
  "icons": {},
  "background": {
    "scripts": [
      "bundle.js"
    ],
    "persistent": false
  },
  "chrome_url_overrides" : {
    "newtab": "index.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "omnibox": {
    "keyword": "aString"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "history",
    "downloads"
  ],
  "version_name": "aString"
}
